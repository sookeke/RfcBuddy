{
  "DataFolder": "./data",
  "SourceInfo": {
    "SourceUrl365": "$(appSetting-source-url)",
    "SourceUrlUser": "$(appSetting-source-user)",
    "SourceUrlPassword": "$(appSetting-source-password)",
    "SourceRefreshMinutes": "60"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "confidential-port": 0,
    "auth-server-url": "$(appSetting-keycloak-server)",
    "realm": "$(appSetting-keycloak-realm)",
    "ssl-required": "external",
    "resource": "$(appSetting-keycloak-resource)",
    "credentials": {
      "secret": "$(appSetting-keycloak-secret)"
    }
  },
  "AllowedHosts": "*"
}
